  # Comment the next line if you don't want to use dynamic frameworks
  use_frameworks!

  # Pods for ParticleWatchiOS WatchKit App
   pod 'Alamofire', '~> 4.8.0'
   pod 'SwiftyJSON', '~> 4.0'

end

target 'ParticleWatchiOS WatchKit Extension' do
  # Comment the next line if you don't want to use dynamic frameworks
  use_frameworks!

  # Pods for ParticleWatchiOS WatchKit Extension
   pod 'Alamofire', '~> 4.8.0'
   pod 'SwiftyJSON', '~> 4.0'

